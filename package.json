    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "faker": "^3.1.0",
    "faucet": "^0.0.1",
    "nodemon": "^1.11.0",
    "nyc": "^10.1.2",
    "supertest": "^3.0.0",
    "tape": "^4.6.3",
    "tape-watch": "^2.2.4"
  },
  "dependencies": {
